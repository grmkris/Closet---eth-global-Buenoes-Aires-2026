{
  "name": "@ai-stilist/api",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "lint": "ultracite check",
    "fix": "ultracite fix",
    "fix:unsafe": "ultracite fix --unsafe",
    "format": "ultracite fix"
  },
  "devDependencies": {
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@orpc/server": "catalog:",
    "@orpc/client": "catalog:",
    "@orpc/openapi": "catalog:",
    "@orpc/zod": "catalog:",
    "hono": "catalog:",
    "dotenv": "catalog:",
    "zod": "catalog:",
    "@ai-stilist/auth": "workspace:*",
    "@ai-stilist/db": "workspace:*"
  }
}
