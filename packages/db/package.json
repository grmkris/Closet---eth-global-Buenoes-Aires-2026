{
  "name": "@ai-stilist/db",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/db.d.ts",
      "default": "./src/db.ts"
    },
    "./schema/auth": {
      "types": "./dist/schema/auth/index.d.ts",
      "default": "./src/schema/auth/index.ts"
    },
    "./schema/auth/index": {
      "types": "./dist/schema/auth/index.d.ts",
      "default": "./src/schema/auth/index.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "lint": "ultracite check",
    "fix": "ultracite fix",
    "fix:unsafe": "ultracite fix --unsafe",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:start": "docker compose up -d",
    "db:watch": "docker compose up",
    "db:stop": "docker compose stop",
    "db:down": "docker compose down"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.2",
    "@types/pg": "^8.11.11",
    "@electric-sql/pglite": "^0.3.10",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-stilist/shared": "workspace:*",
    "@ai-stilist/logger": "workspace:*",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.7.0",
    "pg": "^8.14.1",
    "zod": "catalog:"
  }
}
