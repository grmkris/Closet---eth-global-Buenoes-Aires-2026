{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**"]
		},
		"lint": {
			"dependsOn": ["^lint"],
			"outputs": [],
			"cache": true
		},
		"typecheck": {
			"dependsOn": ["^typecheck"],
			"outputs": [],
			"cache": true
		},
		"check-types": {
			"dependsOn": ["^check-types"],
			"outputs": [],
			"cache": true
		},
		"fix": {
			"outputs": [],
			"cache": false
		},
		"fix:unsafe": {
			"outputs": [],
			"cache": false
		},
		"format": {
			"outputs": [],
			"cache": false
		},
		"test": {
			"cache": false,
			"outputs": ["coverage/**"],
			"env": ["NODE_ENV", "DATABASE_URL"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"db:push": {
			"cache": false,
			"persistent": true
		},
		"db:studio": {
			"cache": false,
			"persistent": true
		},
		"db:migrate": {
			"cache": false,
			"persistent": true
		},
		"db:generate": {
			"cache": false,
			"persistent": true
		},
		"db:start": {
			"cache": false,
			"persistent": true
		},
		"db:stop": {
			"cache": false,
			"persistent": true
		},
		"db:watch": {
			"cache": false,
			"persistent": true
		},
		"db:down": {
			"cache": false,
			"persistent": true
		}
	}
}
